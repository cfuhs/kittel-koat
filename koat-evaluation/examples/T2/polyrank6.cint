(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS f0))
(VAR x0 x1 x2)
(RULES
  f0(x0, x1, x2) -> Com_1(f2(x0, x1, x2)) :|: x0 > -1 && x2 >= x1
  f2(x0, x1, x2) -> Com_1(f2(x0 - 1, x1 - 1, x2)) :|: x0 > 0 && x2 >= x1 - 1
  f2(x0, x1, x2) -> Com_1(f2(x0, x1 - 1, x2 + (x1 - 1))) :|: x0 > -1 && x2 + (x1 - 1) >= x1 - 1
)
