(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS ____start))
(VAR x x__o y y__o z z__o)
(RULES
  start(x, y, z, x__o, y__o, z__o) -> Com_1(stop(x, y, z, x__o, y__o, z__o)) :|: z<=0 && z=z__o && y=y__o && x=x__o 
  start(x, y, z, x__o, y__o, z__o) -> Com_1(lbl_7_1(1  + x, y - 1, z - 1, x__o, y__o, z__o)) :|: z>=1 && z=z__o && y=y__o && x=x__o 
  lbl_7_1(x, y, z, x__o, y__o, z__o) -> Com_1(stop(x, y, z, x__o, y__o, z__o)) :|: z<=0 && x>=x__o+1 && z>=0 && x+z=x__o+z__o && x+y=x__o+y__o 
  lbl_7_1(x, y, z, x__o, y__o, z__o) -> Com_1(lbl_7_1(1  + x, y - 1, z - 1, x__o, y__o, z__o)) :|: z>=1 && x>=x__o+1 && z>=0 && x+z=x__o+z__o && x+y=x__o+y__o 
  ____start(x, y, z, x__o, y__o, z__o) -> Com_1(start(x__o, y__o, z__o, x__o, y__o, z__o)) :|: TRUE 
)
