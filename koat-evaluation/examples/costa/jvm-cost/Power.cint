(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS power))
(VAR A B)
(RULES
 power(A) -> Com_1(m0(A)) :|: TRUE
 m0(A) -> Com_1(m2(A)) :|: TRUE
 m3(A) -> Com_1(m1()) :|: A=0
 m4(B) -> Com_2(m1(),power(A)) :|: A>=0 && B+ -A=1
 m2(A) -> Com_1(m5(A)) :|: TRUE
 m5(A) -> Com_1(m4(A)) :|: TRUE
 m5(A) -> Com_1(m3(A)) :|: TRUE
)
