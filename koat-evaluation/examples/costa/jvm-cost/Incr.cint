(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS add))
(VAR A B C D)
(RULES
 add(A, B) -> Com_1(n4(A, B)) :|: A>=0
 m0(A, B, C) -> Com_1(m1(A, B, C)) :|: TRUE
 m2(A) -> Com_1(m3(A)) :|: TRUE
 m5(B) -> Com_1(m4(A)) :|: B+ -A= -1
 m3(A) -> Com_1(m5(A)) :|: TRUE
 m6(A) -> Com_1(m7(A)) :|: TRUE
 m9(B) -> Com_1(m8(A)) :|: B+ -A= -2
 m7(A) -> Com_1(m9(A)) :|: TRUE
 n0(A) -> Com_1(n1(A)) :|: TRUE
 n3(B) -> Com_1(n2(A)) :|: B+ -A= -3
 n1(A) -> Com_1(n3(A)) :|: TRUE
 n7(A, B, C) -> Com_1(m0(A, B, C)) :|: A+ -C>= -3
 n8(B, C, A) -> Com_2(n7(B, C, D),m2(A)) :|: B+ -A>=0 && A+ -D= -1
 n9(B, C, A) -> Com_2(n7(B, C, D),m6(A)) :|: B+ -A>=0 && A+ -D= -2
 o0(B, C, A) -> Com_2(n7(B, C, D),n0(A)) :|: B+ -A>=0 && A+ -D= -3
 o1(A, B) -> Com_2(n5(),m0(A, B, 0)) :|: A>=0
 n4(A, B) -> Com_1(o1(A, B)) :|: A>=0
 o2(A, B, C) -> Com_1(o3(A, B, C)) :|: A+ -C>=0
 o3(A, B, C) -> Com_1(n8(A, B, C)) :|: TRUE
 o3(A, B, C) -> Com_1(n9(A, B, C)) :|: TRUE
 o3(A, B, C) -> Com_1(o0(A, B, C)) :|: TRUE
 m1(A, B, C) -> Com_1(o2(A, B, C)) :|: TRUE
 m1(A, C, B) -> Com_1(n6(A, B)) :|: TRUE
)
