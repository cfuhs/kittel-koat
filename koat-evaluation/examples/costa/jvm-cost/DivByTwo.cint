(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS divByTwo))
(VAR A B)
(RULES
 divByTwo(A) -> Com_1(m2(A)) :|: A>=1
 m0(A) -> Com_1(m1(A)) :|: TRUE
 m5(A) -> Com_2(m3(),m0(A)) :|: A>=1
 m2(A) -> Com_1(m5(A)) :|: A>=1
 m6(B) -> Com_1(m0(A)) :|: B+ -2*A>=0 && B>=1
 m1(A) -> Com_1(m6(A)) :|: TRUE
 m1(A) -> Com_1(m4(A)) :|: TRUE
)
