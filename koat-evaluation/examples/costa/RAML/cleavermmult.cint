(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS matrixMult))
(VAR A Acc L L1 L2 L1c L1p L2p L1rp L1r L2r L2c Lp L3 Mc Mr Mrp X)
(RULES
 matrixMult(L1r, L1c, L2r, L2c) -> Com_2(computeLine(L1c, L2r, L2c, L3),matrixMult(L1rp, L1c, L2r, L2c)) :|: L1r>=1 && L3=0 && L1rp = L1r - 1
 computeLine(L, Mr, Mc, Acc) -> Com_2(lineMult(X, Mc, Acc),computeLine(Lp, Mrp, Mc, Acc)) :|: L>=1 && Mr>=1 && Lp=L - 1 && Mrp=Mr - 1
 size__lineMult(A, L1, L2) -> Com_1(size__lineMult(A, L1p, L2p)) :|: L1>=1 && L2=0 && L1p = L1 - 1 && L2p=0
 size__lineMult(A, L1, L2) -> Com_1(size__lineMult(A, L1p, L2p)) :|: L1>=1 && L2=0 && L1p = L1 - 1 && L2p=L2 - 1
 lineMult(A, L1, L2) -> Com_1(lineMult(A, L1p, L2p)) :|: L1>=1 && L2=0 && L1p = L1 - 1 && L2p=0
 lineMult(A, L1, L2) -> Com_1(lineMult(A, L1p, L2p)) :|: L1>=1 && L2=0 && L1p = L1 - 1 && L2p=L2 - 1
)
