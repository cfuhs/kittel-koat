(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS lcs))
(VAR A Ap B Bp C Cp E F X)
(RULES
 lcs(A, B) -> Com_1(lcstable(A, B)) :|: A>=0 && B>=0
 lcstable(A, B) -> Com_1(firstline(B)) :|: A=0
 lcstable(A, B) -> Com_2(newline(X, B, B),lcstable(Ap, B)) :|: A>=1 && Ap=A - 1
 out1__lcstable(A, B) -> Com_1(out1__lcstable(Ap, B)) :|: A>=1 && Ap=A - 1
 out2__lcstable(A, B) -> Com_1(out2__lcstable(Ap, B)) :|: A>=1 && Ap=A - 1
 firstline(A) -> Com_1(firstline(Ap)) :|: A >=1 && Ap=A - 1
 newline(A, B, C) -> Com_4(right(Cp),right(Bp),max(E, F),newline(A, Bp, Cp)) :|: C>=1 && B>=1 && Bp=B - 1 && Cp=C - 1
 newline(A, B, C) -> Com_3(right(Cp),right(Bp),newline(A, Bp, Cp)) :|: C>=1 && B>=1 && Bp=B - 1 && Cp=C - 1
 out__newline(A, B, C) -> Com_1(out__newline(A, Bp, Cp)) :|: C>=1 && B>=1 && Bp=B - 1 && Cp=C - 1
)
