(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS isortlist))
(VAR A Ap B Bp C R Rp Rs)
(RULES
 isortlist(R, C) -> Com_2(insert(C, Rp, C),isortlist(Rp, C)) :|: R>=1 && Rp = R - 1
 insert(A, R, C) -> Com_1(leq(A, C)) :|: R>=1
 insert(A, R, C) -> Com_2(leq(A, C),insert(A, Rs, C)) :|: R>=1 && Rs = R - 1
 leq(A, B) -> Com_1(leq(Ap, Bp)) :|: A>=1 && B>=1 && Ap=A - 1 && Bp=B - 1
)
