(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS mergeSort))
(VAR N Np N1 N2 N1p N2p L1 L2)
(RULES
 mergeSort(N) -> Com_4(msplit(N),mergeSort(L1),mergeSort(L2),merge(L1, L2)) :|: N>=2 && L1>=0 && L2>=0 && 2*L1 <= N+1 && 2*L1 >=N && 2*L2<= N && 2*L2 >=N - 1
 out2__msplit(N) -> Com_1(out2__msplit(Np)) :|: N>=2 && Np = N - 2
 out1__msplit(N) -> Com_1(out1__msplit(Np)) :|: N>=2 && Np = N - 2
 msplit(N) -> Com_1(msplit(Np)) :|: N>=2 && Np = N - 2
 merge(N1, N2) -> Com_1(merge(N1p, N2)) :|: N1>=1 && N2>=1 && N1p=N1 - 1
 merge(N1, N2) -> Com_1(merge(N1, N2p)) :|: N1>=1 && N2>=1 && N2p=N2 - 1
)
