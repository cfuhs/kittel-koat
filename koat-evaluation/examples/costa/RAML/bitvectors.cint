(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS mult3))
(VAR A B B1 B1s B2s B2 Bs C I M N R Rp Rs X Y Zs)
(RULES
 mult3(A, B, C) -> Com_2(mult(A, B),mult(A, B)) :|: TRUE
 mult(B1, B2) -> Com_1(mult(B1s, B2)) :|: B1>=1 && B1s = B1 - 1 && Zs >=1+B1s
 mult(B1, B2) -> Com_2(mult(B1s, B2),add(B2, Zs)) :|: B1>=1 && B1s = B1 - 1 && Zs <= B1s+1
 out__mult(B1, B2) -> Com_1(out__mult(B1s, B2)) :|: B1>=1 && B1s=B1 - 1
 out__mult(B1, B2) -> Com_2(out__mult(B1s, B2),outadd(B2, Zs)) :|: B1>=1 && B1s=B1 - 1 && Zs<= B1s+1
 add(B1, B2) -> Com_1(addp(B1, B2, I)) :|: I=0
 bitToInt(A) -> Com_1(bitToIntp(A, I)) :|: I=1
 bitToIntp(B, N) -> Com_1(bitToIntp(Bs, M)) :|: B>=1 && Bs=B - 1 && M=N*2
 addp(B1, B2, R) -> Com_2(sum(X, Y, R),addp(B1s, B2s, Rs)) :|: B1>=1 && B2>=1 && B1s=B1 - 1 && B2s=B2 - 1
 subp(B1, B2, R) -> Com_2(diff(X, Y, R),subp(B1s, B2s, Rp)) :|: B1>=1 && B2>=1 && B1s=B1 - 1 && B2s=B2 - 1
 leq(B1, B2) -> Com_1(compare(B1, B2)) :|: B1>=0 && B2>=0
 compare(B1, B2) -> Com_1(compare(B1s, B2s)) :|: B1>=1 && B2>=1 && B1s=B1 - 1 && B2s=B2 - 1
)
