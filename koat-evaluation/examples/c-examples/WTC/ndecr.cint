(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS eval_ndecr_start))
(VAR v_i.0 v_n)
(RULES
    eval_ndecr_start(v_n) -> Com_1(eval_ndecr_entry_in(v_n))
    eval_ndecr_entry_in(v_n) -> Com_1(eval_ndecr_bb1_in(v_n - 1))
    eval_ndecr_bb1_in(v_i.0) -> Com_1(eval_ndecr_bb_in(v_i.0)) :|: v_i.0 > 1
    eval_ndecr_bb1_in(v_i.0) -> Com_1(eval_ndecr_return_in()) :|: v_i.0 <= 1
    eval_ndecr_bb_in(v_i.0) -> Com_1(eval_ndecr_bb1_in(v_i.0 - 1))
    eval_ndecr_return_in() -> Com_1(eval_ndecr_stop())
)
