(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS l0))
(VAR x r r')
(RULES
 l0(x, r) -> Com_1(l1(x, 0))
 l1(x, r) -> Com_2(l2(x, r), l3(x, r)) :|: x >= 0
 l1(x, r) -> Com_1(l4(x, r)) :|: x < 0

 l2(x, r) -> Com_1(l1(x - 1, r')) :|: x >= 0
 l3(x, r) -> Com_1(l5(x, r'))
 l5(x, r) -> Com_1(l6(x, 1))

 l6(x, r) -> Com_2(l7(x, r), l8(x,r)) :|: x > 0
 l6(x, r) -> Com_1(l9(x, r)) :|: x <= 0
 l7(x, r) -> Com_1(l9(x, r'))

 l8(x, r) -> Com_1(l8(x - 1, r')) :|: x > 0
)
