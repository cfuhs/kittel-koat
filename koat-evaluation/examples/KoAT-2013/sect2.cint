(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS l0))
(VAR x y z t)
(RULES
 l0(x, y, z, t) -> Com_1(l1(x, 0, z, t))
 l1(x, y, z, t) -> Com_1(l1(x - 1, y + 1, z, t)) :|: x > 0
 l1(x, y, z, t) -> Com_1(l2(x, y, y, t)) :|: x <= 0
 l2(x, y, z, t) -> Com_1(l3(x, y, z, z)) :|: z > 0
 l3(x, y, z, t) -> Com_1(l3(x, y, z, t - 1)) :|: t > 0 && z > 0
 l3(x, y, z, t) -> Com_1(l2(x, y, z - 1, t)) :|: t <= 0 && z > 0
)
