(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS eval_1))
(VAR x y z z')
(RULES
  eval_1(x, y) -> Com_1(eval_2(z, z')) :|: x >= 0 && z - (x + 1) >= 0 && x + 1 - z >= 0 && z' - 1 >= 0 && 1 - z' >= 0
  eval_2(x, y) -> Com_1(eval_2(x, z)) :|: x >= 0 && y - 0 - 1 >= 0 && x - y >= 0 && z - (y + 1) >= 0 && y + 1 - z >= 0
  eval_2(x, y) -> Com_1(eval_1(z, y)) :|: x >= 0 && y - 0 - 1 >= 0 && y - x - 1 >= 0 && z - (x - 2) >= 0 && x - 2 - z >= 0
)
