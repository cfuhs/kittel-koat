(GOAL COMPLEXITY)
(STARTTERM CONSTRUCTOR-BASED)
(VAR x y z z')
(RULES
  eval(x, y) -> Com_1(eval(z, x)) :|: x - 0 - 1 >= 0 && y - 0 - 1 >= 0 && z - (x - 1) >= 0 && x - 1 - z >= 0
  eval(x, y) -> Com_1(eval(z, z')) :|: x - 0 - 1 >= 0 && y - 0 - 1 >= 0 && z - (y - 2) >= 0 && y - 2 - z >= 0 && z' - (x + 1) >= 0 && x + 1 - z' >= 0
)
