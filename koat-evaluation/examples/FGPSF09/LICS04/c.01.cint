(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS eval_1))
(VAR x y z)
(RULES
  eval_1(x, y) -> Com_1(eval_2(x, z)) :|: x >= 0 && z - 1 >= 0 && 1 - z >= 0
  eval_2(x, y) -> Com_1(eval_2(x, z)) :|: x >= 0 && y - 0 - 1 >= 0 && x - y - 1 >= 0 && z - 2 * y >= 0 && 2 * y - z >= 0
  eval_2(x, y) -> Com_1(eval_1(z, y)) :|: x >= 0 && y - 0 - 1 >= 0 && y - x >= 0 && z - (x - 1) >= 0 && x - 1 - z >= 0
)
