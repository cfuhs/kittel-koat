(GOAL COMPLEXITY)
(STARTTERM CONSTRUCTOR-BASED)
(VAR x y z)
(RULES
  eval(x, y) -> Com_1(eval(z, y)) :|: x + y - 0 - 1 >= 0 && x - 0 - 1 >= 0 && z - (x - 1) >= 0 && x - 1 - z >= 0
  eval(x, y) -> Com_1(eval(x, z)) :|: x + y - 0 - 1 >= 0 && 0 - x >= 0 && y - 0 - 1 >= 0 && z - (y - 1) >= 0 && y - 1 - z >= 0
  eval(x, y) -> Com_1(eval(x, y)) :|: x + y - 0 - 1 >= 0 && 0 - x >= 0 && 0 - y >= 0
)
