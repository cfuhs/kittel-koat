(GOAL COMPLEXITY)
(STARTTERM CONSTRUCTOR-BASED)
(VAR x z)
(RULES
  eval(x) -> Com_1(eval(z)) :|: x - 0 - 1 >= 0 && 1 - x >= 0 && x % 2 - 0 - 1 >= 0 && z - (x - 1) >= 0 && x - 1 - z >= 0
  eval(x) -> Com_1(eval(z)) :|: x - 0 - 1 >= 0 && 1 - (0 - x) >= 0 && x % 2 - 0 - 1 >= 0 && z - (x - 1) >= 0 && x - 1 - z >= 0
  eval(x) -> Com_1(eval(z)) :|: x - 0 - 1 >= 0 && 1 - x >= 0 && x % 2 >= 0 && 0 - x % 2 >= 0 && z - x / 2 >= 0 && x / 2 - z >= 0
  eval(x) -> Com_1(eval(z)) :|: x - 0 - 1 >= 0 && 1 - (0 - x) >= 0 && x % 2 >= 0 && 0 - x % 2 >= 0 && z - x / 2 >= 0 && x / 2 - z >= 0
)
