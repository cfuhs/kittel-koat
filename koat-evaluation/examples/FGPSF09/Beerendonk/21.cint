(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS eval_1))
(VAR x y z z' z'')
(RULES
  eval_1(x, y, z) -> Com_1(eval_2(x, y, z)) :|: x - y >= 0 && y - x >= 0 && x - z - 1 >= 0
  eval_2(x, y, z) -> Com_1(eval_2(z', z'', z)) :|: y - z - 1 >= 0 && z' - (x - 1) >= 0 && x - 1 - z' >= 0 && z'' - (y - 1) >= 0 && y - 1 - z'' >= 0
  eval_2(x, y, z) -> Com_1(eval_1(x, y, z)) :|: z - y >= 0
)
