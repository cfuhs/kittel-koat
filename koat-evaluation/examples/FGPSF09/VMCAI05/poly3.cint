(GOAL COMPLEXITY)
(STARTTERM CONSTRUCTOR-BASED)
(VAR x y z z' z'')
(RULES
  eval(x, y, z) -> Com_1(eval(z', z'', z)) :|: x >= 0 && z * z * z - y >= 0 && z' - (x - 1) >= 0 && x - 1 - z' >= 0 && z'' - (y - 1) >= 0 && y - 1 - z'' >= 0
  eval(x, y, z) -> Com_1(eval(x, z', z'')) :|: x >= 0 && z * z * z - y >= 0 && z' - (y - 1) >= 0 && y - 1 - z' >= 0 && z'' - (z + y) >= 0 && z + y - z'' >= 0
)
