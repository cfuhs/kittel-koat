(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS random))
(VAR x z y f)
(RULES
  random(x) -> Com_1(rand(x, z)) :|: z >= 0 && 0 - z >= 0
  rand(x, y) -> Com_0 :|: x >= 0 && 0 - x >= 0
  rand(x, y) -> Com_2(rand(z, f), id_inc(y)) :|: x - 0 - 1 >= 0 && z - (x - 1) >= 0 && x - 1 - z >= 0
  rand(x, y) -> Com_2(rand(z, f), id_dec(y)) :|: 0 - x - 1 >= 0 && z - (x + 1) >= 0 && x + 1 - z >= 0
  id_inc(x) -> Com_0 :|: TRUE
  id_inc(x) -> Com_0 :|: z - (x + 1) >= 0 && x + 1 - z >= 0
  id_dec(x) -> Com_0 :|: TRUE
  id_dec(x) -> Com_0 :|: z - (x - 1) >= 0 && x - 1 - z >= 0
)
