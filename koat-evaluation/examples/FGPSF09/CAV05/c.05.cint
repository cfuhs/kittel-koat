(GOAL COMPLEXITY)
(STARTTERM CONSTRUCTOR-BASED)
(VAR x y z)
(RULES
  eval(x, y) -> Com_1(eval(z, y)) :|: x - y - 1 >= 0 && x - 0 - 1 >= 0 && y - 0 - 1 >= 0 && z - (x - y) >= 0 && x - y - z >= 0
  eval(x, y) -> Com_1(eval(z, y)) :|: y - x - 1 >= 0 && x - 0 - 1 >= 0 && y - 0 - 1 >= 0 && x - y - 1 >= 0 && z - (x - y) >= 0 && x - y - z >= 0
  eval(x, y) -> Com_1(eval(x, z)) :|: x - y - 1 >= 0 && x - 0 - 1 >= 0 && y - 0 - 1 >= 0 && y - x >= 0 && z - (y - x) >= 0 && y - x - z >= 0
  eval(x, y) -> Com_1(eval(x, z)) :|: y - x - 1 >= 0 && x - 0 - 1 >= 0 && y - 0 - 1 >= 0 && y - x >= 0 && z - (y - x) >= 0 && y - x - z >= 0
)
