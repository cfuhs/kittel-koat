(GOAL COMPLEXITY)
(STARTTERM CONSTRUCTOR-BASED)
(VAR x y z z')
(RULES
  eval(x, y, z) -> Com_1(eval(z', y, z)) :|: y - x - 1 >= 0 && z - x - 1 >= 0 && z' - (x + 1) >= 0 && x + 1 - z' >= 0
  eval(x, y, z) -> Com_1(eval(x, y, z')) :|: y - x - 1 >= 0 && x - z >= 0 && z' - (z + 1) >= 0 && z + 1 - z' >= 0
)
